@model IEnumerable<MVCDemo2.Models.Alarme>

@{
    ViewBag.Title = "AlarmesdoContrato";
}

<h3>Alarmes do Contrato de @ViewBag.Nome em @ViewBag.Data</h3>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnderecoInstalacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataInstalacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contrato.Dados)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnderecoInstalacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataInstalacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contrato.Dados)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
